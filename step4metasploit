import os
import time
from pymetasploit3.msfrpc import *

# start msf remote api
os.system("msfrpcd -P testpw -S")
time.sleep(5) # pause for the service to fully start
client = MsfRpcClient('testpw', port=55553, ssl=False) # make connection

cid = client.consoles.console().cid # create console and get Id
client.consoles.console(cid).write('workspace') # write as if in msf console
print(client.consoles.console(cid).read()) # read results from writing to console

# close msfrpcd
os.system("killall ruby")




if(client.modules.use('exploit', exploit).rank is "excellent"):
    print(exploit + "  " + client.modules.use('exploit', exploit).rank)
